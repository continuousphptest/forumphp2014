<VirtualHost *:80>
	ServerAdmin webmaster@localhost

    SetEnv APPLICATION_ENV "development"
    DocumentRoot /vagrant/public

	<Directory />
		Options FollowSymLinks
		AllowOverride None
	</Directory>
	<Directory /vagrant>
	    Header set Access-Control-Allow-Origin "*"
		Options FollowSymLinks MultiViews
		AllowOverride All
		Order allow,deny
		allow from all
	</Directory>
</VirtualHost>